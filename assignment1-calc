#include<stdio.h>
#include<stdlib.h>

int arr[100];
char op[100];
int idx=0,opidx=0;

int prior(char op){
    if(op=='+' || op=='-') return 1;
    if(op=='*' || op=='/') return 2;
    return 0;
}
void apply(){
    if (idx < 2 || opidx < 1) {
        printf("Error: Invalid expression.\n");
        return;
    }
    int a= arr[--idx];
    //printf("a %d\n",a);
    int b= arr[--idx];
    //printf("b %d\n",b);
    char c= op[--opidx];
    //printf("c %c\n",c);
    // printf("%d %d %s ",a,b,c);
    if(c=='+') arr[idx++]=a+b;
    else if(c=='-') arr[idx++]=b-a;
    else if(c=='*') arr[idx++]=a*b;
    else if (c == '/') {
        if (a == 0) {
            printf("Error: Division by zero.\n");
            return;
        }
        arr[idx++] = b / a;
    } 
    else {
        printf("invalid \n");
        return;
    } 
    return;
}
int calc(char *exp){
    int ans =0;
    int i=0;
    int curr=0;
    
    while(exp[i] != '\0'){
        if(exp[i]==' ') {
            i++;
            continue;
        }
        else if (exp[i]>='0' && exp[i]<='9'){
            // int j=i;
            curr=0;
            while(exp[i]>='0' && exp[i]<='9'){
                curr = curr*10 + (exp[i]-'0');
                i++;
            }
            i--;
            arr[idx++]=curr;
            curr=0;
        }
        else if(exp[i]=='+' || exp[i]=='-' || exp[i]=='*' || exp[i]=='/'){
            while(opidx>0 &&  prior(exp[i]) <= prior(op[opidx - 1])){
                apply();
            }
            op[opidx++]=exp[i];
        }
        i++;
    }
    printf("opidx %d\n",opidx);
    while(opidx > 0) {
        apply();
    }
    if(idx !=1){
        printf("invalid exp");
        exit(1);
    }
    return arr[--idx];
}
int main(){
    char exp[20];
    scanf("%[^\n]%*c",exp); 
    int ans = 0;
    ans = calc(exp);
    printf("%d",ans);
    return 1;
}
